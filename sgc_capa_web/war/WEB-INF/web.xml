<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
    
  <servlet>
    <servlet-name>usuarioServlet</servlet-name>
    <servlet-class>py.progweb.fpuna.server.UsuarioServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>usuarioServlet</servlet-name>
    <url-pattern>/sgc_capa_web/usuarioService</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Sgc_capa_web.html</welcome-file>
  </welcome-file-list>

</web-app>
